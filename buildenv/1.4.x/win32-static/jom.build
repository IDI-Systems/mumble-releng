urls=(
	"http://releng-distfiles.mumble.info/releng-distfiles/jom_1_1_3.zip"
	"http://download.qt.io/official_releases/jom/jom_1_1_3.zip"
)

digests=(
	"sha1:30d7e15d76d3ed04206b6e23f9ce08237eedb77e"
	"sha256:128fdd846fe24f8594eed37d1d8929a0ea78df563537c0c1b1861a635013fff8"
