function extract {
	unzip -q -o -d jom_1_1_3 jom_1_1_3.zip
	cd jom_1_1_3
}

function build {
	:
}

function install {
	mkdir -p ${MUMBLE_PREFIX}/jom/bin/
	cp jom.exe ${MUMBLE_PREFIX}/jom/bin/jom.exe
	chmod +rx ${MUMBLE_PREFIX}/jom/bin/jom.exe
}
